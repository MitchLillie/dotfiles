// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "shift+cmd+s",
        "command": "workbench.action.files.save"
    },
    {
        "key": "cmd+s",
        "command": "-workbench.action.files.save"
    },
    {
        "key": "cmd+s",
        "command": "workbench.action.files.saveAll"
    },
    {
        "key": "alt+cmd+s",
        "command": "-workbench.action.files.saveAll"
    },
    {
        "key": "cmd+f9",
        "command": "workbench.action.zoomReset"
    }
,{
    "key": "shift+cmd+d",
    "command": "commandId",
    "when": "editorTextFocus"
}]